<% layout('layout')%>
<div class="mb-1 text-center">
<h3>Overview table</h3> 
</div>
<div class="p-2">
   
   <table class="table table-sm table-responsive-sm table-bordered table table-striped table-hover">
        <thead>
            <tr>
                <th scope="col">Date</th>
                <th scope="col">From</th>
                <th scope="col">To</th>
                <th scope="col">Start</th>
                <th scope="col">Finish</th>
                <th scope="col">Distance</th>
                <th scope="col">Project</th>
              </tr>
        </thead>
        <tbody>
            <% for (let timesheet of timesheets) { %> 
            <tr>
                <th><%=timesheet.createdAt%></th>
                <td><%=timesheet.from%></td>
                <td><%=timesheet.to%></td>
                <td><%=timesheet.start%></td>
                <td><%=timesheet.finish%></td>
                <td><%=timesheet.finish - timesheet.start%></td>
                <td><%=timesheet.project%></td>
            </tr>
            <% } %> 
        </tbody>
    </table>
</div>
